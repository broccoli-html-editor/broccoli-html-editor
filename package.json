  "license": "MIT",
  "bugs": {
    "url": "https://github.com/broccoli-html-editor/broccoli-html-editor/issues"
  },
  "homepage": "https://github.com/broccoli-html-editor/broccoli-html-editor#readme",
  "dependencies": {
    "cheerio": "0.22.0",
    "ejs": "^2.6.2",
    "es6-promise": "^3.0.2",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14",
    "htmlparser": "^1.7.7",
    "iterate79": "^1.1.1",
    "jquery": "^3.6.0",
    "langbank": "github:tomk79/node-langbank.git#develop",
    "marked": "^4.0.12",
    "mime-types": "^2.1.35",
    "mkdirp": "^0.5.1",
